{
  "version": 2,
  "buildCommand": "npx nx build teddy-finance",
  "outputDirectory": "dist/apps/teddy-finance",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/clientes/(.*)", "destination": "/" },
    { "source": "/clientes-selecionados/(.*)", "destination": "/" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
